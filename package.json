    "prisma": "^5.11.0",
    "prosekit": "^0.6.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.10.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^1.0.2",
    "rehype-katex": "^6.0.3",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.6.0",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "styled-components": "^6.1.0",
    "tailwindcss": "^3.4.1",
    "unist-util-visit": "^4.1.0"
