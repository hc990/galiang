    "@pothos/plugin-prisma": "3.65.0",
    "@prisma/client": "5.11.0",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.13",
    "axios": "^1.5.1",
    "esbuild": "0.18.11",
    "github-slugger": "^2.0.0",
    "graphql": "16.8.1",
    "graphql-scalars": "1.23.0",
    "graphql-yoga": "5.2.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
